<div class ="container">
  <div class ="row">

    <div class ="col-md-10">
      <table class="table table-hover table-inverse">
        <tr>
          <th>title</th>
          <th>text</th>
          <th>日時</th>
        </tr>
        <% @posts.each do |post| %>
        <tr>
          <td><%= post.title %></td>
          <td><%= post.text %></td>
          <td><%= post.created_at.strftime('%m/%d %H:%M') %></td>
        </tr>
        <% end %>
      </table>
    </div>

  </div>
</div>